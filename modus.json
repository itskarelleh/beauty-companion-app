{
  "$schema": "https://schema.hypermode.com/modus.json",
  "endpoints": {
    "default": {
      "type": "graphql",
      "path": "/graphql",
      "auth": "bearer-token"
    }
  },
  "models": {
    "any-to-any": {
      "sourceModel": "deepseek-ai/Janus-1.3B",
      "provider": "hugging-face",
      "connection": "hypermode"
    },
    "text-generation": {
      "sourceModel": "google/gemma-2-2b-it",
      "provider": "hugging-face",
      "connection": "hypermode"
    },
    "mask-generation": {
      "sourceModel": "deepseek-ai/Janus-1.3B",
      "provider": "hugging-face",
      "connection": "hypermode"
    }
  }
}
